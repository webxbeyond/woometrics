# Server Configuration
PORT=9090
LOG_LEVEL=info

# Cron schedule for metrics collection (every 5 minutes)
SCRAPE_INTERVAL=*/5 * * * *

# Store 1 Configuration
STORE1_NAME=My WooCommerce Store 1
STORE1_URL=https://your-store1.com
STORE1_CONSUMER_KEY=ck_your_consumer_key_here
STORE1_CONSUMER_SECRET=cs_your_consumer_secret_here
STORE1_ENABLED=true
STORE1_CURRENCY=USD
STORE1_SCRAPE_INTERVAL=300000
STORE1_TIMEOUT=30000
STORE1_MAX_RETRIES=3

# Store 2 Configuration
STORE2_NAME=My WooCommerce Store 2
STORE2_URL=https://your-store2.com
STORE2_CONSUMER_KEY=ck_your_consumer_key_here
STORE2_CONSUMER_SECRET=cs_your_consumer_secret_here
STORE2_ENABLED=true
STORE2_CURRENCY=EUR
STORE2_SCRAPE_INTERVAL=300000
STORE2_TIMEOUT=30000
STORE2_MAX_RETRIES=3

# Store 3 Configuration
STORE3_NAME=My WooCommerce Store 3
STORE3_URL=https://your-store3.com
STORE3_CONSUMER_KEY=ck_your_consumer_key_here
STORE3_CONSUMER_SECRET=cs_your_consumer_secret_here
STORE3_ENABLED=false
STORE3_CURRENCY=GBP
STORE3_SCRAPE_INTERVAL=300000
STORE3_TIMEOUT=30000
STORE3_MAX_RETRIES=3

# Add more stores as needed (STORE4_, STORE5_, etc.)
# The application will automatically detect and load all configured stores

# Optional: Node Environment
NODE_ENV=production