  },
  "bugs": {
    "url": "https://github.com/your-org/woometrics/issues"
  },
  "dependencies": {
    "@woocommerce/woocommerce-rest-api": "^1.0.2",
    "express": "^4.18.2",
    "prom-client": "^15.0.0",
